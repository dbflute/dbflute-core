/* table supplementary info */
span.tableschematitle {
	font-size: 0.5em;
	color: #888888;
}
span.tablefirstdatetitle {
	font-size: 0.5em;
	color: #888888;
}

/* -------------------------------------------------
                                    Arrange Query
------------------------------------------------- */
span.arrqycount {
	font-size: 0.5em;
}
dl.arrqylist {
	margin-top: 0.7em;
	padding-left: 3em;
