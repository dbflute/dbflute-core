	 */
	fetchDecomment: function(successCallback) {
		var path = '/api/document/decomment/' + DecommentUtil.getSchemaName() + '/pickup';
		var method = 'GET';
		var errorCallback = function() {
			alert('Error!! cannot save decomment.');
		}
